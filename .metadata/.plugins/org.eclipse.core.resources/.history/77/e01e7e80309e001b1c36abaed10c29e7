package entity;

public class Pet {
	private int pet_id;
	private int pet_type_id;
	private int pet_breed_id;
	private String pet_name;
	private String pet_type_name;
	private String pet_breed_name;
	
	public Pet(int pet_id, int pet_type_id, int pet_breed_id, String pet_name, String pet_type_name, String pet_breed_name) {
		this.setPetID(pet_id);
		this.setPetName(pet_name)
	}
}
